{
  "name": "plantilla-kuntica-nuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "prebuild": "test \"$CI\" = true && npx pnpm install -r --store=node_modules/.pnpm-store || echo skiping pnpm install",
    "build:start": "npm run build && npm run start",
    "generate:start": "npm run generate && npm run start",
    "lint:js": "eslint --ext .js,.vue,.ts .",
    "lint:style": "stylelint **/*.{vue,css}",
    "lint": "npm run lint:js && npm run lint:style",
    "test": "npm run test:unit && npm run test:e2e:ci",
    "test:unit": "cross-env TEST=unit jest",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:ci": "start-server-and-test generate:start http://localhost:3000 test:e2e",
    "export-tailwind-config": "tailwindcss-export-config --config=tailwind.config.js --destination=assets/styles/abstract/_variables --format=scss --quoted-keys=true",
    "docommit": "commit"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/vue-fontawesome": "2",
    "@nuxt/content": "1.10.0",
    "@nuxt/typescript-runtime": "2.0.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "core-js": "3.6.5",
    "d3": "^6.7.0",
    "node-fetch": "2.6.1",
    "nuxt": "2.14.7",
    "property-information": "^5.6.0",
    "remark": "^13.0.0",
    "sass": "^1.27.0",
    "start-server-and-test": "^1.11.5",
    "tailwindcss-export-config": "^2.3.0",
    "vue-client-only": "2.0.0",
    "vue-head": "2.2.0",
    "vue-meta": "2.4.0",
    "vue-router": "3.4.8"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "17.0.0",
    "@commitlint/prompt-cli": "^11.0.0",
    "@cypress/webpack-preprocessor": "^5.4.8",
    "@nuxt/types": "2.14.7",
    "@nuxt/typescript-build": "2.0.3",
    "@nuxtjs/eslint-module": "3.0.0",
    "@nuxtjs/stylelint-module": "4.0.0",
    "@nuxtjs/tailwindcss": "3.2.0",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@tailwindcss/typography": "^0.2.0",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@typescript-eslint/parser": "4.6.0",
    "@vue/server-test-utils": "^1.1.0",
    "@vue/test-utils": "1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.1",
    "babel-plugin-module-resolver": "4.0.0",
    "cross-env": "7.0.2",
    "cypress": "^5.5.0",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-standard": "15.0.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.2",
    "eslint-plugin-unicorn": "23.0.0",
    "eslint-plugin-vue": "7.1.0",
    "eslint-webpack-plugin": "2.1.0",
    "fibers": "^5.0.0",
    "husky": "4.3.0",
    "jest": "26.6.1",
    "jsdom": "16.4.0",
    "jsdom-global": "3.0.2",
    "lint-staged": "10.5.0",
    "prettier": "2.1.2",
    "sass-loader": "^10.0.4",
    "stylelint": "13.7.2",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-scss": "^3.18.0",
    "tailwindcss": "^1.9.6",
    "ts-jest": "26.4.3",
    "typescript": "4.0.5",
    "vue": "^2.6.12",
    "vue-gtag": "^1.10.0",
    "vue-jest": "3.0.7",
    "vue-no-ssr": "^1.1.1",
    "vue-server-renderer": "^2.6.12",
    "vue-template-compiler": "2.6.12",
    "webpack": "^4.44.2"
  }
}
